{
  "expo": {
    "name": "ER Simulator",
    "slug": "ersimulator",
    "version": "1.0.0",
    "scheme": "ersimulator",
    "platforms": [
      "ios",
      "android"
    ],
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-router"
    ],
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#133A3E"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#133A3E"
      },
      "package": "com.ersimulator.app",
      "bundleIdentifier": "com.ersimulator.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#133A3E"
      },
      "package": "com.ersimulator.app",
      "edgeToEdgeEnabled": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "21fcce4f-5adb-4a70-bdf9-9a59da38a424"
      }
    }
  }
}
